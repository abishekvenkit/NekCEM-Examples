cat rea.top  > qq.rea
cat rea.nel >> qq.rea
cat rea.xy  >> qq.rea
cat rea.cs  >> qq.rea
cat rea.bot >> qq.rea
echo qq > SESSION.NAME

pretex << EOF
qqc
   1 READ PREVIOUS PARAMETERS 
qq
   1 BUILD FROM FILE          
qq
   1 END    ELEMENTS          
   1 ACCEPT MATL,QVOL         
   1 SET BCs                  
  11 SET ENTIRE LEVEL         
   8 SYMMETRY                 
   1 END  LEVEL               
   1 SET BCs                  
   1 END  LEVEL               
   1 EXIT                     
EOF

echo qqc > SESSION.NAME
prex &

